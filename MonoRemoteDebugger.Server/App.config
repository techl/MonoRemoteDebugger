<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <appSettings>
    <add key="ServerPort" value="13001"/>
    <add key="DebuggerAgentPort" value="11000"/>
    <!-- path to pdb2mdb.exe -->
    <add key="LibMonoApplicationPath" value="/usr/lib/mono/4.5/"/>
    <!-- path to install a bash script 'pdb2mdb' to call 'mono /usr/lib/mono/4.5/pdb2mdb.exe' under (embedded) linux -->
    <add key="ShellScriptInstallPath" value="/usr/bin/"/>
    <!-- don't delete the last X debug content directories -->
    <add key="SkipLastUsedContentDirectories" value="3"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>